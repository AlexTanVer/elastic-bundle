{
  "name": "alextanver/elastic-bundle",
  "type": "symfony-bundle",
  "description": "Elastic Bundle",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-curl": "*",
    "symfony/dotenv": "*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "*",
    "symfony/yaml": "*",
    "elasticsearch/elasticsearch": "^8.3",
    "symfony/console": "*"
  },
  "autoload": {
    "psr-4": {
      "AlexTanVer\\ElasticBundle\\": "src"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
